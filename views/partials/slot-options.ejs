<%
slotOptions = function(slotNumber){
const slots = ["1", "2", "3", "4", "5", "6", "7", "8"]
let optionsString = ''
slots.forEach(slot => {
  const selected = slotNumber == slot ? "selected" : ""
  const option = `<option value="${slot}" ${selected}>${slot}</option>`;
  optionsString += option
})
return optionsString}
%>
